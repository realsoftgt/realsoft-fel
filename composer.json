{
  "name": "realsoft/realsoft-fel",
  "description": "FEL/DTE multi-paises (GT/SV) con drivers de certificador (Infile) y validación de esquemas.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Henry Leonardo Díaz Gutiérrez",
      "email": "hdiaz@realsoft.dev"
    }
  ],
  "homepage": "https://packages.realsoft.dev/fel",
  "keywords": [
    "laravel",
    "FEL",
    "DTE",
    "Guatemala",
    "El Salvador",
    "Infile",
    "e-invoicing",
    "facturación electrónica",
    "electronic invoicing",
    "realsoftgt",
    "RealSoft",
    "Intelguasoft",
    "Galilea",
    "ERP",
    "Contabilidad",
    "Accounting"
  ],
  "require": {
    "php": "^8.1|^8.2|^8.3",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.8",
    "opis/json-schema": "^2.5"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0|^10.0",
    "pestphp/pest": "^2.0",
    "laravel/pint": "^1.13",
    "phpstan/phpstan": "^1.11",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "RealSoft\\FEL\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RealSoft\\FEL\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RealSoft\\FEL\\FELServiceProvider"
      ],
      "aliases": {
        "FEL": "RealSoft\\FEL\\Facades\\FEL"
      }
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
